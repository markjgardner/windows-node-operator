apiVersion: v1
kind: Pod
metadata:
  name: add-reg-key2
  namespace: default
spec:
  containers:
  - args:
    - /c
    - reg
    - add
    - HKLM\SYSTEM\CurrentControlSet\Services\hns\State
    - /v
    - DNSMaximumTTL
    - /t
    - REG_DWORD
    - /d
    - "30"
    - /f
    command:
    - cmd.exe
    image: mcr.microsoft.com/windows/nanoserver:ltsc2019
    name: add-reg-key
  tolerations:
  - effect: NoSchedule
    key: node/freshness
    value: new