{
    "metadata": {
        "name": "add-reg-key",
        "namespace": "default"
    },
    "spec": {
        "containers": [
            {
                "args": [
                    "/c",
                    "/c", 
                    "reg", 
                    "add", 
                    "HKLM\\SYSTEM\\CurrentControlSet\\Services\\hns\\State", 
                    "/v",
                    "DNSMaximumTTL",
                    "/t",
                    "REG_DWORD", 
                    "/d",
                    "30", 
                    "/f"
                ],
                "command": [
                    "cmd.exe"
                ],
                "image": "mcr.microsoft.com/oss/kubernetes/windows-host-process-containers-base-image:v1.0.0",
                "name": "add-reg-key",
                "securityContext": {
                    "privileged": true
                }
            }
        ],
        "nodeName": "akswin1000000"
    }
}